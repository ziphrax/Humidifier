{
  "profiles": {
    "deploy-base-$env$": {
      "commandName": "Project",
      "commandLineArgs": "deploy-base --env=$env$"
    },
    "deploy-simple-$env$": {
      "commandName": "Project",
      "commandLineArgs": "deploy-simple --env=$env$"
    },
    "invoke-simple-$env$": {
      "commandName": "Project",
      "commandLineArgs": "invoke-simple --env=$env$"
    },
    "destroy-base-$env$": {
      "commandName": "Project",
      "commandLineArgs": "destroy-base --env=$env$"
    },
    "destroy-simple-$env$": {
      "commandName": "Project",
      "commandLineArgs": "destroy-simple --env=$env$"
    }
  }
}